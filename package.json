{
  "name": "insurance-eligibility-automation",
  "version": "1.0.0",
  "description": "Automated insurance eligibility verification for BlueSprig clinics",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test":   "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=src/__tests__/patientCsv\\.test\\.js",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=src/__tests__/.*\\.int\\.test\\.js",
    "test:e2e": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=src/__tests__/.*\\.e2e\\.test\\.js"
  },
  "keywords": [
    "automation",
    "healthcare",
    "rpa",
    "browser-automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "playwright": "^1.56.1"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.6.1",
    "luxon": "^3.7.2"
  }
}
